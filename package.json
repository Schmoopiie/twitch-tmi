{
  "name": "tmi.js",
  "version": "1.8.5",
  "description": "Javascript library for the Twitch Messaging Interface.",
  "keywords": [
    "tmi",
    "tmijs",
    "twitch",
    "chat",
    "message",
    "messaging",
    "interface",
    "bot"
  ],
  "main": "index.js",
  "files": [
    "lib",
    "dist"
  ],
  "engines": {
    "node": ">=16.11.0"
  },
  "author": "Schmoopiie",
  "scripts": {
    "test": "run-s eslint test:*",
    "eslint": "eslint index.js lib/* test/*",
    "eslint-fix": "eslint --fix index.js lib/* test/*",
    "test:mocha": "nyc mocha -- --require should --exit",
    "build": "run-s build:*",
    "build:rimraf": "rimraf ./dist",
    "build:mkdirp": "mkdirp ./dist",
    "build:dedupe": "npm dedupe",
    "build:esbuild": "node scripts/build.js",
    "build:sri": "node scripts/sri.js ./dist/tmi.js",
    "build:sri-min": "node scripts/sri.js ./dist/tmi.min.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tmijs/tmi.js.git"
  },
  "bugs": {
    "url": "https://github.com/tmijs/tmi.js/issues"
  },
  "dependencies": {
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "eslint": "8.57.0",
    "hook-std": "2.0.0",
    "mkdirp": "3.0.1",
    "mocha": "9.2.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rimraf": "5.0.7",
    "should": "13.2.3"
  }
}
